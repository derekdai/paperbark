#!/bin/bash
commands['list']='list stagging filesystems'
command_alias['ls']='list'


list_help() {
    echo """usage:
    $exec_file list
"""

    exit 0
}

cmd_list_prepare() {
    if [[ '-h' == "$1" || '--help' == "$1" ]]; then
        list_help
    fi

    return
}

cmd_list_exec() {
    _list_exec() {
        printf "%s\n" "$1"
    }

    echo STAGGING FS
    stagging_enum _list_exec
}
