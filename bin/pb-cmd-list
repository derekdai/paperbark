#!/bin/bash
commands['list']='list stagging filesystems'
command_alias['ls']='list'


list_help() {
    echo """usage:
    $exec_file list|ls [OPTIONS]

where OPTIONS are
    -h | --help
        this help message
    -s | --script
        script mode, do not show column header
"""

    exit 0
}

cmd_list_prepare() {
    while [[ 0 != $# ]]; do
        case "$1" in
            -h | '--help')
                list_help
                ;;
            -s | '--script')
                list_script_mode=yes
                ;;
        esac
        shift
    done
}

cmd_list_exec() {
    _list_exec() {
        printf "%s\n" "$1"
    }

    if [[ -z "$list_script_mode" ]]; then
        echo STAGGING FS
    fi
    stagging_enum _list_exec
}
