#!/bin/bash
commands['list-snapshot']='list snapshots'
command_alias['ls-snap']='list-snapshot'


list_snapshot_help() {
    echo """usage:
    $exec_file list-snapshot
"""

    exit 0
}

cmd_list_snapshot_prepare() {
    if [[ '-h' == "$1" || '--help' == "$1" ]]; then
        list_help
    fi

    return
}

cmd_list_snapshot_exec() {
    _list_snapshot_exec() {
        printf "%-20s    %-15s    %s\n" "$1" "$2" "$3"
    }

    printf "%-20s    %-15s    %s\n" NAME VERSION FS
    snapshot_enum _list_snapshot_exec
}
