#!/bin/bash
commands['push']='push snapshot from client to server'

cmd_push_prepare() {
    target_snaps=("$@")
}

cmd_push_exec() {
    empty_callback() {
        return
    }

    snapshot_series_gen_diff target_snaps "${target_snaps[@]}"

    snapshot_receive_all empty_callback "${target_snaps[@]}"
}

